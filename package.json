{
  "name": "ecl-fruver",
  "version": "1.0.0",
  "description": "Sistema de Facturaci√≥n para ECL FRUVER",
  "main": "server.js",
  "bin": "server.js",
  "pkg": {
    "assets": [
      "views/**/*",
      "public/**/*",
      "node_modules/ejs/**/*",
      "node_modules/bootstrap/dist/**/*",
      "node_modules/jquery/dist/**/*",
      "node_modules/sweetalert2/dist/**/*",
      "node_modules/select2/dist/**/*",
      "node_modules/select2-bootstrap-5-theme/dist/**/*",
      "node_modules/bootstrap-icons/font/**/*"
    ],
    "targets": [
      "node18-win-x64"
    ],
    "outputPath": "dist"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "pkg . --public --target node18-win-x64"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "body-parser": "^1.20.2",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "dotenv": "^16.6.1",
    "ejs": "^3.1.9",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-session": "^1.18.2",
    "jquery": "^3.7.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.14.2",
    "qrcode": "^1.5.4",
    "select2": "^4.1.0-rc.0",
    "select2-bootstrap-5-theme": "^1.3.0",
    "sweetalert2": "^11.15.10"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "pkg": "^5.8.1"
  }
}
